<p id="notice"><%= notice %></p>

<h1>Listing Albumfotorestorans</h1>

<table>
  <thead>
    <tr>
      <th>Restaurant</th>
      <th>Foto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @albumfotorestorans.each do |albumfotorestoran| %>
      <tr>
        <td><%= albumfotorestoran.restaurant_id %></td>
        <td><%= albumfotorestoran.foto %></td>
        <td><%= link_to 'Show', albumfotorestoran %></td>
        <td><%= link_to 'Edit', edit_albumfotorestoran_path(albumfotorestoran) %></td>
        <td><%= link_to 'Destroy', albumfotorestoran, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Albumfotorestoran', new_albumfotorestoran_path %>
